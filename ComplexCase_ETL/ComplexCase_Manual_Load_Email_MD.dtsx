      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;DECLARE @tab char(1) = CHAR(9) &#xA;EXEC msdb.dbo.sp_send_dbmail @profile_name=NULL,&#xA;@body_format='html',@body='&quot;+ @[User::HtmlEmailBody] +&quot;',&#xA;@recipients= '&quot; + @[$Project::ToLine]  + &quot;',&#xA;@subject='&quot; + @[$Project::HtmlEmailSubject]   + &quot;',&#xA;@execute_query_database = 'CERRS_ODS',&#xA;@attach_query_result_as_file = 1,&#xA;@query_attachment_filename = 'CCHCManualCasesReport.csv',&#xA;@query_result_width = 150,&#xA;@query_result_separator=@tab,&#xA;@query_result_no_padding = 1,&#xA;@query_result_header= 1,&#xA;@query = 'SET NOCOUNT ON;&#xA;select&#xA;  LTRIM(REPLACE(ISNULL(ComplexCaseNumber,''''), char(9), '''')) as ComplexCaseNumber&#xA;,ApplicationID&#xA;,LTRIM(REPLACE(ISNULL(HICSCaseID,''''), char(9), '''')) as HICSCaseID&#xA;,LTRIM(REPLACE(ISNULL(AnalystName,''''), char(9), '''')) as AnalystName&#xA;,LTRIM(REPLACE(ISNULL(CaseType,''''), char(9), '''')) as CaseType&#xA;,Case when [CRM_ComplexCaseGUID] is not null then ''Yes''&#xA;else ''No'' end as LoadedToCRM&#xA;,convert(nvarchar(MAX), CreateDateUtc, 20) As CreateDateUtc&#xA;FROM [dbo].[HICSComplexCase]&#xA;where (isnull(Manual,''N'') = ''Y'' )&#xA;and CreateDateUtc &gt;''&quot;+ (DT_STR,30, 1252)@[User::LastRunDate] +&quot;'''&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="strSqlDbSendMail">
      <DTS:VariableValue
        DTS:DataType="8">DECLARE @tab char(1) = CHAR(9) 
EXEC msdb.dbo.sp_send_dbmail @profile_name=NULL,
@body_format='html',@body='&lt;h1&gt;CCHC Manual Report&lt;/h1&gt;',
@recipients= 'Piyush.Goyal@cognosante.com; Daniel.Thonn@cognosante.com',
@subject='CCHC Manual Cases Report',
@execute_query_database = 'CERRS_ODS',
@attach_query_result_as_file = 1,
@query_attachment_filename = 'CCHCManualCasesReport.csv',
@query_result_width = 150,
@query_result_separator=@tab,
@query_result_no_padding = 1,
@query_result_header= 1,
@query = 'SET NOCOUNT ON;
select
  LTRIM(REPLACE(ISNULL(ComplexCaseNumber,''''), char(9), '''')) as ComplexCaseNumber
,ApplicationID
,LTRIM(REPLACE(ISNULL(HICSCaseID,''''), char(9), '''')) as HICSCaseID
,LTRIM(REPLACE(ISNULL(AnalystName,''''), char(9), '''')) as AnalystName
,LTRIM(REPLACE(ISNULL(CaseType,''''), char(9), '''')) as CaseType
,Case when [CRM_ComplexCaseGUID] is not null then ''Yes''
else ''No'' end as LoadedToCRM
,convert(nvarchar(MAX), CreateDateUtc, 20) As CreateDateUtc
FROM [dbo].[HICSComplexCase]
where (isnull(Manual,''N'') = ''Y'' )
and CreateDateUtc &gt;''1/1/1900'''</DTS:VariableValue>
